{
  "manifest_version": 2,

  "name": "Jira Log Time",
  "description": "Quickly log Jira time",
  "version": "1.0",

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "storage",
   "<all_urls>"
   ]
}
